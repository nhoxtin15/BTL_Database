<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="350.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="database_btl.Employee.model.Receipt.Checkout.Controller.ChooseCustomerController">
   <children>
      <VBox fillWidth="false" prefWidth="300.0" spacing="20.0">
         <children>
            <HBox alignment="TOP_CENTER" prefHeight="20.0" prefWidth="300.0">
               <children>
                  <Label text="Customer Chooser">
                     <font>
                        <Font name="System Bold" size="16.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <VBox pickOnBounds="false" prefHeight="62.0" prefWidth="212.0" spacing="10.0" VBox.vgrow="NEVER">
               <children>
                  <HBox alignment="CENTER_LEFT" prefHeight="26.0" prefWidth="208.0" spacing="5.0">
                     <children>
                        <Label prefWidth="35.0" text="Phone" />
                        <TextField fx:id="PhoneToFind" prefWidth="150.0" />
                     </children>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                     <padding>
                        <Insets left="10.0" />
                     </padding>
                  </HBox>
                  <HBox alignment="CENTER_LEFT" prefHeight="26.0" prefWidth="204.0" spacing="5.0">
                     <children>
                        <Label prefWidth="35.0" text="SSN" />
                        <TextField fx:id="SSNToFind" prefWidth="150.0" />
                     </children>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                     <padding>
                        <Insets left="10.0" />
                     </padding>
                  </HBox>
               </children>
            </VBox>
            <HBox alignment="TOP_RIGHT" prefHeight="26.0" prefWidth="280.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#findCustomer" text="Find Customer" />
               </children>
               <VBox.margin>
                  <Insets right="20.0" />
               </VBox.margin>
            </HBox>
            <Line endX="300.0" strokeWidth="2.0" />
            <VBox pickOnBounds="false" prefHeight="128.0" prefWidth="288.0" spacing="10.0">
               <children>
                  <HBox alignment="TOP_CENTER">
                     <children>
                        <Label text="Customer">
                           <font>
                              <Font name="System Bold" size="16.0" />
                           </font>
                        </Label>
                     </children>
                  </HBox>
                  <HBox alignment="CENTER_LEFT" prefHeight="26.0" prefWidth="208.0" spacing="5.0">
                     <children>
                        <Label prefWidth="35.0" text="Name" />
                        <TextField fx:id="CustomerFound" editable="false" prefWidth="150.0" />
                     </children>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                     <padding>
                        <Insets left="10.0" />
                     </padding>
                  </HBox>
                  <HBox alignment="CENTER_LEFT" prefHeight="26.0" prefWidth="294.0" spacing="5.0">
                     <children>
                        <Label prefWidth="35.0" text="Point" />
                        <TextField fx:id="PointFound" editable="false" prefWidth="150.0" />
                     </children>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                     <padding>
                        <Insets left="10.0" />
                     </padding>
                  </HBox>
                  <HBox alignment="TOP_RIGHT" prefWidth="300.0" spacing="10.0">
                     <children>
                        <Button mnemonicParsing="false" onAction="#cancel" text="Cancel" />
                        <Button mnemonicParsing="false" onAction="#register" text="Register" />
                        <Button mnemonicParsing="false" onAction="#chooseCustomer" prefHeight="26.0" prefWidth="125.0" text="Choose Customer" />
                     </children>
                     <VBox.margin>
                        <Insets right="2.0" />
                     </VBox.margin>
                  </HBox>
               </children>
            </VBox>
         </children>
         <padding>
            <Insets top="10.0" />
         </padding>
      </VBox>
   </children>
</Pane>
