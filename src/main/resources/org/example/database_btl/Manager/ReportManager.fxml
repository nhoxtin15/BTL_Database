            <Insets left="20.0" right="20.0" />
         </padding>
         <children>
            <Button mnemonicParsing="false" text="Yearly">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" text="Monthly">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" text="Quarterly" onAction="#getReportQuarter">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" text="Week">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
